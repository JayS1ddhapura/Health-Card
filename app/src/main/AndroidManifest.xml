<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hackathonproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Newhealth"></activity>
        <activity android:name=".Labreport" />
        <activity android:name=".labReportDisplay" />
        <activity android:name=".medicineDisplay" />
        <activity android:name=".casesDisplay" />
        <activity
            android:name=".ui.slideshow.SubActivity"
            android:label="@string/title_activity_sub"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.hackathonproject.MainActivity" />
        </activity>
        <activity
            android:name=".Healthhistory"
            android:parentActivityName=".Hackathonnav" />
        <activity android:name=".Forgotpwd" />
        <activity
            android:name=".Hackathonnav"
            android:label="@string/title_activity_hackathonnav"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Regsuccess" />
        <activity android:name=".Signup" />
        <activity android:name=".Login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Medicine"
            android:parentActivityName=".Hackathonnav" />
    </application>

</manifest>